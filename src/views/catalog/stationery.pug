extends ../../mixins/catalog
include ../../mixins/catalogList 
include ../../mixins/topbar 

block head
  - var title = catalog.stationery.name + " | Богатый Мир Упаковки"
  - var description = "Богатый Мир Упаковки. Упаковка, Хозяйственные, Канцелярские товары, Подарки"
  - var keywords = "ручка, карандаши, дырокол, степлеры, ценники, линейки, скрепки, корректор, факс, бумага, ролики, блокноты, скотч, документы, доверенности, папки, оптовая база, пленка, стрейч, туалетная бумага, ложки, ножи, вилки, бытовая химия, для мытья посуды, для мытья стекол, бахилы, ценники, бумага, блокноты, тетради,  ПНД, ПВД, пакеты майка,   термоусадочные пленки, скотч, упаковка, упаковочка, одноразовая посуда, бму"
  +head(title, description, keywords)

block topbar
  +topbar('catalog', catalog.stationery.name)
  
block content
  +catalogList(catalog.stationery)
