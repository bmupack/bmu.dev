extends ../../mixins/catalog
include ../../mixins/catalogList 
include ../../mixins/topbar 

block head
  - var title = catalog.celebration.name + " | Богатый Мир Упаковки"
  - var description = "Богатый Мир Упаковки. Упаковка, Хозяйственные, Канцелярские товары, Подарки"
  - var keywords = "подарки, упаковка, для подарков, оформление, свадьба, машины, игры, призы, праздничная, свадебная, оптовая база, пленка, стрейч, туалетная бумага, ложки, ножи, вилки, бытовая химия, для мытья посуды, для мытья стекол, бахилы, ценники, бумага, блокноты, тетради,  ПНД, ПВД, пакеты майка,   термоусадочные пленки, скотч, упаковка, упаковочка, одноразовая посуда, бму"
  +head(title, description, keywords)

block topbar
  +topbar('catalog', catalog.celebration.name)

block content
  +catalogList(catalog.celebration)
